Algoritmo "consumo"


Var

   dist, combust, consumo : real

Inicio

      escreva("Distância percorrida: ")
      leia(dist)
      escreva("Combustível gasto: ")
      leia(combust)
      
      consumo <- dist/combust
      
      escreval("Consumo médio = ", consumo:8:3)


Fimalgoritmo